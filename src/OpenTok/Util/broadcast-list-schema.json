{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "type":"object",
    "properties": {
      "count": {
        "description": "Total number of broadcasts of the query",
        "type": "integer"
      },
      "items": {
        "type": "array",
        "items": {
          "title": "Broadcast",
          "description": "An OpenTok Broadcast",
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "pattern": "^[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}$"
            },
            "sessionId": {
              "type": "string"
            },
            "projectId": {
              "type": "integer"
            },
            "createdAt": {
              "type" : "integer"
            },
            "updatedAt": {
              "type" : "integer"
            },
            "resolution": {
              "type": "string"
            },
            "broadcastUrls": {
              "oneOf": [
                {
                  "type": "null"
                },
                {
                  "type": "object",
                  "properties": {
                    "hls": {
                      "type": "string"
                    }
                  }
                }
              ]
            }
          },
          "required": ["id", "sessionId", "projectId", "createdAt", "updatedAt"]
        }
      }
    },
    "required": ["count", "items"]
  }